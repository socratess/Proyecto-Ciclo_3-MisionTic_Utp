@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";

}

<section>
   <div class="col-12 col-md-6 slide carousel-fade" > 
    <div id="carouselExampleInterval" class="carousel slide" data-bs-ride="carousel">
  <div class="carousel-inner">
  
    <div class="carousel-item active" data-bs-interval="2000">
      <img src="~/img/mascota4.jpg" class="d-block w-100" alt="mascota4" height="336px">
    </div>
    <div class="carousel-item" data-bs-interval="2000">
      <img src="~/img/mascota2.jpg" class="d-block w-100" alt="mascota2" height="336px">
    </div>
    <div class="carousel-item">
      <img src="~/img/mascota3.jpg" class="d-block w-100" alt="mascota3" height="336px">
    </div>
    <div class="carousel-item">
      <img src="~/img/mascota1.jpg" class="d-block w-100" alt="mascota1" height="336px">
    </div>
  </div>

  </div>
  <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
</div>
</div>
</div>
</section>
