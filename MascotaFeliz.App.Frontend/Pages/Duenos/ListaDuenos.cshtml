@page
@model MascotaFeliz.App.Frontend.Pages.ListaDuenosModel
@{
}
  <div>
<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/Index">Inicio</a></li>
    <li class="breadcrumb-item" aria-current="page">Propietario</li>
    <li class="breadcrumb-item active" aria-current="page">Consulta De Propietarios</li>
  </ol>
</nav>
</div> 


<div  id="listaDiseno" class="container mt-5">
<table id="tableDueno" class="table table-striped" style="width:100%">
  <thead>
   <th>Cedula</th>
     <th>Nombres</th>
     <th>Apellidos</th>
     <th>Direccion</th>
     <th>Telefono</th>
     <th>Correo</th>
     <th>Ver Detalles</th>
     <th>Editar</th>
     <th>Eliminar</th>
       
  </thead>
  <tbody>
    @foreach (var dueno in Model.listaDuenos){

    <tr>
      <td>@dueno.Cedula</td>
        <td>@dueno.Nombres</td>
        <td>@dueno.Apellidos</td>
           <td>@dueno.Direccion</td>
              <td>@dueno.Telefono</td>
                 <td>@dueno.Correo</td>
                 <td>
                    <a class="btn btn-primary table-btn" asp-page="/Duenos/DetallesDuenos" asp-route-duenoId="@dueno.Id">Detalle</a>
                 </td>
                 <td>
                    <a class="btn btn-primary table-btn" asp-page="/Duenos/EditarDuenos" asp-route-duenoId="@dueno.Id">Editar</a>
                 </td>
                 <td>
                    <a class="btn btn-primary table-btn" asp-page="/Duenos/EliminarDuenos" asp-route-duenoId="@dueno.Id">Eliminar</a>
                 </td>
    </tr>
     }
  </tbody>
</table>
</div>

</table>